&lt;td width=&quot;73&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;strong&gt;DEBE&lt;/strong&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;73&quot; valign=&quot;middle&quot;&gt;
