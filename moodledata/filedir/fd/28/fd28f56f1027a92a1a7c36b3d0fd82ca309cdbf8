           <int value="2"></int>
           <string role="key" value="last_full_node_path"></string>
           <string value="exe-node:UNIDAD%20DID%C3%81CTICA%202.%20TRAMITACI%C3%93N%20DE%20OPERACIONES%20B%C3%81SICAS%20DE%20COBROS%20Y%20PAGOS:2.%20Descripci%C3%B3n%20De%20Medios%20De%20Pago:2.1.%20Tarjetas%20De%20D%C3%A9bito"></string>
