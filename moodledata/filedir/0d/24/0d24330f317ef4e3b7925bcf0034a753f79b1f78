      <instance class="exe.engine.node.Node" reference="26">
       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="5. Reglamentación Europea: Reglamento (CE) N.° 1005/2009 Del Parlamento Europeo, Directiva 2006/40/CE Del Parlamento Europeo, Reglamento (CE) N.° 842/2006"></unicode>
        <string role="key" value="idevices"></string>
