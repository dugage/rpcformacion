        <unicode value="1. La Tesorería Neta Y El Coeficiente Básico De Financiación (Capital Corriente Real Y Capital Corriente Mínimo)"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="2">
