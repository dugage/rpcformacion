              <string value="exe-node:Tema%204.Impuesto%20sobre%20la%20Renta%20de%20las%20Personas%20F%C3%ADsicas%20%28IRPF%29.%20I:2.Elementos%20del%20impuesto:3.Rendimientos%20de%20actividades%20econ%C3%B3micas:3.1.Concepto%20y%20delimitaci%C3%B3n"></string>
              <string role="key" value="parent"></string>
              <reference key="19"></reference>
              <string role="key" value="top_anchors_linked_from_fields"></string>
