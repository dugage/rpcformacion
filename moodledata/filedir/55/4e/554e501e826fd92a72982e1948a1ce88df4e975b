&lt;/td&gt;
&lt;td width=&quot;62&quot; height=&quot;37&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;HABER&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;/tr&gt;
&lt;tr valign=&quot;top&quot;&gt;
&lt;td height=&quot;37&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;Deterioro de valor del inmovilizado material (291)&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;56&quot; height=&quot;37&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;-&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;62&quot; height=&quot;37&quot; valign=&quot;middle&quot;&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;tr valign=&quot;top&quot;&gt;
&lt;td height=&quot;37&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;Deterioro de valor de existencias (39)&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;56&quot; height=&quot;37&quot; valign=&quot;middle&quot;&gt;&lt;/td&gt;
&lt;td width=&quot;62&quot; height=&quot;37&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;-&lt;/p&gt;
&lt;/td&gt;
&lt;/tr&gt;
