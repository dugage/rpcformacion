           <string value="exe-node:Tema%20%202.%20M%C3%A9todos%20de%20Valoraci%C3%B3n%20de%20Inmuebles:2.M%C3%A9todo%20de%20capitalizaci%C3%B3n%20de%20las%20rentas%20para%20el%20c%C3%A1lculo%20del%20valor%20en%20venta:2.3%20Los%20M%C3%A9todos%20De%20Capitalizaci%C3%B3n%2C%20Directa%20E%20Indirecta%2C%20De%20Las%20Rentas%20Obtenidas"></string>
