           <string value="exe-node:PROGRAMACI%C3%93N%20Y%20ORGANIZACI%C3%93N%20DE%20ACTIVIDADES%20DE%20FITNESS%20COLECTIVO%20CON%20SOPORTE%20MUSICAL:1.%20Fundamentos-marco%20te%C3%B3rico:1.3.%20Oferta%20de%20actividades%20necesarias%20e%20introducci%C3%B3n%20de%20actividades%20de%20tendencias%20actuales"></string>
