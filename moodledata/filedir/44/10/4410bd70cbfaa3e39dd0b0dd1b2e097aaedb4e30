          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="Ley 58/2003"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="155">
