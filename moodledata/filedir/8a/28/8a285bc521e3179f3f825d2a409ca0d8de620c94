&lt;/td&gt;
&lt;td width=&quot;72&quot; height=&quot;48&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;(1.500)&lt;/p&gt;
&lt;/td&gt;
&lt;/tr&gt;
&lt;/tbody&gt;
&lt;/table&gt;
