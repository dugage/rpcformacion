           <string role="key" value="_title"></string>
           <unicode value="3.3. Congelación"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="49">
             <dictionary>
              <string role="key" value="_title"></string>
