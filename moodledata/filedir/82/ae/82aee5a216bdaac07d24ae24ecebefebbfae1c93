&lt;td style=&quot;border-width: 1px; border-color: #b1bbcc; border-style: solid; background-color: #dfe3e8;&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;Nombre del recurso&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td style=&quot;border-width: 1px; border-color: #b1bbcc; border-style: solid; background-color: #dfe3e8;&quot; valign=&quot;middle&quot; width=&quot;56&quot;&gt;
&lt;p&gt;&lt;b&gt;Tipo&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td style=&quot;border-width: 1px; border-color: #b1bbcc; border-style: solid; background-color: #dfe3e8;&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;Tasa estándar&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td style=&quot;border-width: 1px; border-color: #b1bbcc; border-style: solid; background-color: #dfe3e8;&quot; valign=&quot;middle&quot; width=&quot;95&quot;&gt;
