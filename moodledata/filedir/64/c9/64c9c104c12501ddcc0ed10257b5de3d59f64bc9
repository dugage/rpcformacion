&lt;td valign=&quot;middle&quot;&gt;
&lt;p&gt;x+2&lt;/p&gt;
&lt;/td&gt;
&lt;td valign=&quot;middle&quot;&gt;
&lt;p&gt;1.600&lt;/p&gt;
&lt;/td&gt;
&lt;td valign=&quot;middle&quot;&gt;
&lt;p&gt;2.200&lt;/p&gt;
&lt;/td&gt;
&lt;td valign=&quot;middle&quot;&gt;
&lt;p&gt;600-400=200&lt;/p&gt;
