           <unicode value="5.7. Gastos financieros"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="40">
             <dictionary>
              <string role="key" value="_title"></string>
