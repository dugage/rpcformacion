&lt;p align=&quot;justify&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;Por el cobro:&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;table cellpadding=&quot;5&quot; cellspacing=&quot;0&quot; style=&quot;width: 330px;&quot;&gt;&lt;colgroup&gt;&lt;col width=&quot;185&quot; /&gt; &lt;col width=&quot;58&quot; /&gt; &lt;col width=&quot;55&quot; /&gt; &lt;/colgroup&gt;
