           <string role="key" value="anchor_fields"></string>
           <list></list>
           <string role="key" value="exe.engine.node.Node.persistenceVersion"></string>
           <int value="2"></int>
           <string role="key" value="last_full_node_path"></string>
           <string value="exe-node:UNIDAD%20DID%C3%81CTICA%208.%20COMUNICACI%C3%93N%20ESCRITA:3.T%C3%A9cnica%20y%20normas%20gramaticales:3.1.%20Signos%20De%20Puntuaci%C3%B3n"></string>
