      <instance class="exe.engine.node.Node" reference="24">
       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="Autoevaluación"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.quiztestidevice.QuizTestIdevice" reference="25">
          <dictionary>
