        <string role="key" value="exe.engine.node.Node.persistenceVersion"></string>
        <int value="2"></int>
        <string role="key" value="exportType"></string>
        <string value="scorm"></string>
        <string role="key" value="last_full_node_path"></string>
        <string value="exe-node:Tema%203%20.%20Impuesto%20sobre%20el%20Valor%20A%C3%B1adido%20%28IVA%29.%20II:4.Deducci%C3%B3n%20de%20las%20cuotas%20soportadas"></string>
        <string role="key" value="parent"></string>
