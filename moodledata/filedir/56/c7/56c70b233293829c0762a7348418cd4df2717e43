          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="1.6.2. El empresario"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="66">
             <dictionary>
