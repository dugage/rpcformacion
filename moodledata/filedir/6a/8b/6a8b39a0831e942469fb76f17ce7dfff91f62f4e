           <string value="exe-node:Inicio:TEMA%2014.%20INTELIGENCIA%20EMOCIONAL%20APLICADA%20A%20LAS%20T%C3%89CNICAS%20DE%20VENTA:4.%20Facilidades%20de%20la%20Inteligencia%20Emocional"></string>
