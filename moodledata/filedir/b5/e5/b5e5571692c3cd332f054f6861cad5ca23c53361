&lt;p align=&quot;justify&quot; style=&quot;text-indent: 0.39cm; margin-top: 0.39cm; margin-bottom: 0cm; line-height: 171%;&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span size=&quot;2&quot; style=&quot;font-size: small;&quot;&gt;&lt;b&gt;&lt;span style=&quot;background: #f4f4f4;&quot;&gt;Por los gastos de protesto:&lt;/span&gt;&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
