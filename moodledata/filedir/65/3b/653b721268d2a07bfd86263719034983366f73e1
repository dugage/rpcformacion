       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="Las Dimensiones Del Desarrollo"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="12">
          <dictionary>
