&lt;p align=&quot;center&quot; style=&quot;margin-top: 0.39cm;&quot;&gt;&lt;span color=&quot;#33a3ff&quot; style=&quot;color: #33a3ff;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span style=&quot;font-size: 10pt;&quot;&gt;&lt;b&gt;Haber&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
