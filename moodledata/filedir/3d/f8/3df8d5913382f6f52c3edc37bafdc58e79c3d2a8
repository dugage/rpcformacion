        <string role="key" value="exe.engine.node.Node.persistenceVersion"></string>
        <int value="2"></int>
        <string role="key" value="last_full_node_path"></string>
        <string value="exe-node:UNIDAD%20DID%C3%81CTICA%201.%20OPERACIONES%20DE%20COMPRA%20DE%20ALIMENTOS%20Y%20PRODUCTOS%20PARA%20EL%20DOMICILIO.:3.%20Proceso%20De%20Elaboraci%C3%B3n%20De%20La%20Lista%20De%20La%20Compra"></string>
