       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="1. Arrendamiento Para Uso Distinto Al De La Vivienda."></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="5">
