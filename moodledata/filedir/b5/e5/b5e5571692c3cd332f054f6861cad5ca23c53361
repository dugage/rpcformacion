&lt;td width=&quot;39&quot; bgcolor=&quot;#e6e6e6&quot;&gt;
&lt;p&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;44&quot; bgcolor=&quot;#e6e6e6&quot;&gt;
&lt;p&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span&gt;&lt;b&gt;-&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
