       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="3. El miedo en la infancia"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="23">
          <dictionary>
           <string role="key" value="_title"></string>
