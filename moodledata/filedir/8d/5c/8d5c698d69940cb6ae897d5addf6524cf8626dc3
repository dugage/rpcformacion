           <unicode value="2.2. Hipocampo"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="17">
             <dictionary>
              <string role="key" value="_title"></string>
