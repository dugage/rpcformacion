           <unicode value="3.3. Terapia familiar"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="57">
             <dictionary>
              <string role="key" value="_title"></string>
