           <unicode value="4.4.Cuota tributaria"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="55">
             <dictionary>
              <string role="key" value="_title"></string>
