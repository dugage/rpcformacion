&lt;/td&gt;
&lt;td colspan=&quot;2&quot; width=&quot;177&quot; height=&quot;32&quot; valign=&quot;middle&quot;&gt;&lt;/td&gt;
&lt;td height=&quot;32&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;HABER&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;3&quot;&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;tr valign=&quot;top&quot;&gt;
&lt;td colspan=&quot;2&quot; width=&quot;273&quot; height=&quot;53&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;Aumentos de activo&lt;/p&gt;
&lt;/td&gt;
&lt;td colspan=&quot;3&quot; width=&quot;275&quot; height=&quot;53&quot; valign=&quot;middle&quot;&gt;
