       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="2. Procedimientos De Organización Y Almacenamiento De Alimentos, Productos Y Enseres"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="10">
          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="Free Text"></unicode>
