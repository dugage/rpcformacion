&lt;td width=&quot;171&quot; bgcolor=&quot;#e0e0e0&quot;&gt;
&lt;p align=&quot;justify&quot;&gt;&lt;span color=&quot;#33a3ff&quot; style=&quot;color: #33a3ff;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;BAI &lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
