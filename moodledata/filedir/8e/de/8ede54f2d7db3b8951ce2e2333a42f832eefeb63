         <instance class="exe.engine.node.Node" reference="142">
          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="15.1. Hemorragia externa"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="140">
             <dictionary>
              <string role="key" value="_title"></string>
