              <unicode value="7.1.2. Fracción Insaponificable"></unicode>
              <string role="key" value="idevices"></string>
              <list>
               <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="115">
                <dictionary>
                 <string role="key" value="_title"></string>
