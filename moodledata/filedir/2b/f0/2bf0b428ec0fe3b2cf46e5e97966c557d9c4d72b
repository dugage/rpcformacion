&lt;p&gt;-&lt;/p&gt;
&lt;/td&gt;
&lt;/tr&gt;
&lt;/tbody&gt;
&lt;/table&gt;
&lt;ul&gt;
&lt;li&gt;Si fuesen fondos externos:&lt;/li&gt;
&lt;/ul&gt;
&lt;table width=&quot;524&quot; border=&quot;1&quot; cellpadding=&quot;4&quot; cellspacing=&quot;-1&quot;&gt;
&lt;tbody&gt;
&lt;tr valign=&quot;top&quot;&gt;
&lt;td height=&quot;47&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;CONCEPTO&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;71&quot; height=&quot;47&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;DEBE&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;71&quot; height=&quot;47&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;HABER&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
