        <string role="key" value="_title"></string>
        <unicode value="1. Medidas de prevención de riesgos, de protección medioambiental y de seguridad y de salud laboral en instalaciones deportivas:"></unicode>
        <string role="key" value="idevices"></string>
        <list>
