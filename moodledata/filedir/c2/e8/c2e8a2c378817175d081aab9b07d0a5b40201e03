       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="6. La marca"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="29">
          <dictionary>
           <string role="key" value="_title"></string>
