           <unicode value="6.4. Tipos Y Fuentes De Contaminación En El Interior De Los Edificios"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="106">
