           <unicode value="5.5. Gastos de personal"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="34">
             <dictionary>
              <string role="key" value="_title"></string>
