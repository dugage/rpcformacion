           <string value="exe-node:FACTORES%20DE%20LA%20CONDICI%C3%93N%20F%C3%8DSICA%20Y%20MOTRIZ:2.%20Capacidades%20F%C3%ADsicas%20y%20Rendimiento%20Atl%C3%A9tico%20%28concepto%20tradicional%29:2.1.%20Resistencia%3A%20Aer%C3%B3bica-anaer%C3%B3bica"></string>
