      <instance class="exe.engine.node.Node" reference="22">
       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="3. Modelos explicativos de las altas capacidades"></unicode>
        <string role="key" value="idevices"></string>
        <list>
