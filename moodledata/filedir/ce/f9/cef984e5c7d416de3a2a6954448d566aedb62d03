&lt;p align=&quot;justify&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span&gt;&lt;b&gt;&lt;span&gt;Por el inventario de las existencias finales:&lt;/span&gt;&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;table cellpadding=&quot;5&quot; cellspacing=&quot;0&quot; style=&quot;width: 451px;&quot;&gt;&lt;colgroup&gt;&lt;col width=&quot;312&quot; /&gt; &lt;col width=&quot;54&quot; /&gt; &lt;col width=&quot;53&quot; /&gt; &lt;/colgroup&gt;
&lt;tbody&gt;
&lt;tr&gt;
&lt;td width=&quot;312&quot; bgcolor=&quot;#e6e6e6&quot;&gt;
&lt;p align=&quot;center&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span&gt;&lt;b&gt;CONCEPTO&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;54&quot; bgcolor=&quot;#e6e6e6&quot;&gt;
&lt;p align=&quot;center&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span&gt;&lt;b&gt;DEBE&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
