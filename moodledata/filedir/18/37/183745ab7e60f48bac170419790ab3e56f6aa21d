           <string value="exe-node:Tema%209.%20Activos%20Financieros:2.%20Pr%C3%A9stamos%20Y%20Partidas%20A%20Cobrar%3A%20Clientes%20Y%20Deudores:2.1%20Registro"></string>
           <string role="key" value="parent"></string>
