           <string role="key" value="_title"></string>
           <unicode value="3.3. Mercados de servicios"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="20">
             <dictionary>
              <string role="key" value="_title"></string>
