        <list>
         <instance class="exe.engine.node.Node" reference="33">
          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="4.1. Precedencias y tratamientos de autoridades"></unicode>
           <string role="key" value="idevices"></string>
           <list>
