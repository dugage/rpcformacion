           <string value="exe-node:TEMA%202.%20ORGANIZACI%C3%93N%20Y%20GESTI%C3%93N%20HOSPITALARIA:6.%20La%20habitaci%C3%B3n%20hospitalaria:6.1.%20Mobiliario%20y%20accesorios%20de%20la%20habitaci%C3%B3n%20hospitalaria"></string>
           <string role="key" value="parent"></string>
           <reference key="32"></reference>
           <string role="key" value="top_anchors_linked_from_fields"></string>
           <list></list>
           <string role="key" value="children"></string>
           <list></list>
          </dictionary>
         </instance>
         <instance class="exe.engine.node.Node" reference="38">
          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="6.2. La cama hospitalaria"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="36">
             <dictionary>
