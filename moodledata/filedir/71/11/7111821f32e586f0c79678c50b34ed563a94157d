       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="3. Modificar una imagen"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="9">
