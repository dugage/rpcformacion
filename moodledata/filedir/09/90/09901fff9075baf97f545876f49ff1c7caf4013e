           <string role="key" value="_title"></string>
           <unicode value="4.2. SAAC Con Ayuda Externa: Sistemas E Imágenes, Sistemas Pictográficos (Spc), Bliss, Otros ﻿﻿﻿﻿﻿﻿"></unicode>
           <string role="key" value="idevices"></string>
