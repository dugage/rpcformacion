              <unicode value="4.1. Metabolismo de los aminoácidos en el músculo durante el ejercicio"></unicode>
              <string role="key" value="idevices"></string>
              <list>
               <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="50">
