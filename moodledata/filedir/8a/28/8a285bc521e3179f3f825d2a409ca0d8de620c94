       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="Glosario"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="62">
          <dictionary>
           <string role="key" value="_title"></string>
