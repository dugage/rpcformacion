         <instance class="exe.engine.node.Node" reference="182">
          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="18.1. Clasificación de las quemaduras según su gravedad"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="180">
             <dictionary>
              <string role="key" value="_title"></string>
              <unicode value="Free Text"></unicode>
              <string role="key" value="_author"></string>
