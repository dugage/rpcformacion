         </instance>
         <instance class="exe.engine.node.Node" reference="65">
          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="3.4.Cuota tributaria"></unicode>
           <string role="key" value="idevices"></string>
           <list>
