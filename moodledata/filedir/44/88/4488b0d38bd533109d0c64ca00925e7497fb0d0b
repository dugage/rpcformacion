       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="2. Analítica Web"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="6">
