      <instance class="exe.engine.node.Node" reference="74">
       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="8. Sistema Endocrino Y Hormonas"></unicode>
        <string role="key" value="idevices"></string>
        <list>
