          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="1.4. Las pagas extraordinarias"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="13">
             <dictionary>
              <string role="key" value="_title"></string>
