       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="Materiales de ambientación"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="15">
