           <unicode value="5.1.Interés de demora"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="61">
             <dictionary>
              <string role="key" value="_title"></string>
