        <unicode value="4. Valor Calórico De Los Alimentos"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="49">
          <dictionary>
           <string role="key" value="_title"></string>
