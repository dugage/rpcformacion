           <unicode value="2.4. Ligamentos Que Refuerzan La Capsula Articular De La Articulación De La Rodilla"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="27">
