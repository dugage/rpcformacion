           <unicode value="12. Comedores escolares"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="59">
             <dictionary>
              <string role="key" value="_title"></string>
