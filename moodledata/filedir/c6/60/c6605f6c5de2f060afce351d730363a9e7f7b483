        <string value="exe-node:%20UNIDAD%20DID%C3%81CTICA%202.%20PROTOCOLO%20B%C3%81SICO%20Y%20NORMAS%20DE%20COMPORTAMIENTO%20EN%20RESTAURACI%C3%93N:8.Planos%20de%20mesa%20y%20mesero"></string>
        <string role="key" value="parent"></string>
