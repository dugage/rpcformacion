&lt;td width=&quot;81&quot; height=&quot;41&quot; valign=&quot;middle&quot;&gt;&lt;/td&gt;
&lt;td width=&quot;84&quot; height=&quot;41&quot; valign=&quot;middle&quot;&gt;
