                 <string role="key" value="_title"></string>
                 <unicode value="4.2.4. Flúor"></unicode>
                 <string role="key" value="idevices"></string>
                 <list>
                  <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="56">
