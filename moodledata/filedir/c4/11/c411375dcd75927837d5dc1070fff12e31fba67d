         <instance class="exe.engine.node.Node" reference="68">
          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="5.1. Valoración De Menús Según El Número Y Requisitos Alimenticios Específicos De Los Comensales (Alergias. Niños, Personas Mayores, Dietas)."></unicode>
           <string role="key" value="idevices"></string>
