&lt;p align=&quot;justify&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span&gt;&lt;b&gt;&lt;span&gt;Al cierre del ejercicio, los asientos relativos a existencias serán:&lt;/span&gt;&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;table cellpadding=&quot;5&quot; cellspacing=&quot;0&quot; style=&quot;width: 461px;&quot;&gt;&lt;colgroup&gt;&lt;col width=&quot;339&quot; /&gt; &lt;col width=&quot;40&quot; /&gt; &lt;col width=&quot;50&quot; /&gt; &lt;/colgroup&gt;
