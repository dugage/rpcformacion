        <string value="exe-node:Tema%204.Impuesto%20sobre%20la%20Renta%20de%20las%20Personas%20F%C3%ADsicas%20%28IRPF%29.%20I:6.Ganancias%20y%20p%C3%A9rdidas%20patrimoniales"></string>
        <string role="key" value="parent"></string>
