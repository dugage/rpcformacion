       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="11. Supervisión y control del desarrollo del evento"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="33">
          <dictionary>
           <string role="key" value="_title"></string>
