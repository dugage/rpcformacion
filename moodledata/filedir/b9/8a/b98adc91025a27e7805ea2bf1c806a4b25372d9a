           <unicode value="3.3. Efecto Térmico De Los Alimentos"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="31">
             <dictionary>
              <string role="key" value="_title"></string>
