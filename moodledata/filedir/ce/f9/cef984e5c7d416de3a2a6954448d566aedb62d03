&lt;p align=&quot;justify&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span&gt;&lt;b&gt;50&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
