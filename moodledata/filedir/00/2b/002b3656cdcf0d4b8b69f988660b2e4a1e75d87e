           <unicode value="5.2. Lateralidad"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="60">
             <dictionary>
              <string role="key" value="_title"></string>
