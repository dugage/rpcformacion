&lt;p&gt;&lt;b&gt;CONCEPTO&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;95&quot; height=&quot;41&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;DEBE&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;95&quot; height=&quot;41&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;HABER&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;/tr&gt;
&lt;tr valign=&quot;top&quot;&gt;
&lt;td height=&quot;41&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;Compras de… (60)&lt;/p&gt;
&lt;p&gt;HP IVA soportado (472)&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;95&quot; height=&quot;41&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;-&lt;/p&gt;
&lt;p&gt;-&lt;/p&gt;
