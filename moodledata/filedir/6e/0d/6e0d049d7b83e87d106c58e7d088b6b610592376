           <string role="key" value="exe.engine.node.Node.persistenceVersion"></string>
           <int value="2"></int>
           <string role="key" value="last_full_node_path"></string>
           <string value="exe-node:TEMA%209%20%20%3A%20EL%20VENDEDOR.%20PSICOLOG%C3%8DA%20APLICADA:8.%20Actitud%20y%20comunicaci%C3%B3n%20no%20verbal:8.4.%20La%20prox%C3%A9mica"></string>
           <string role="key" value="parent"></string>
           <reference key="40"></reference>
