              <list></list>
              <string role="key" value="exe.engine.node.Node.persistenceVersion"></string>
              <int value="2"></int>
              <string role="key" value="exportType"></string>
              <string value="scorm"></string>
              <string role="key" value="last_full_node_path"></string>
              <string value="exe-node:Tema%204.Impuesto%20sobre%20la%20Renta%20de%20las%20Personas%20F%C3%ADsicas%20%28IRPF%29.%20I:2.Elementos%20del%20impuesto:3.Rendimientos%20de%20actividades%20econ%C3%B3micas:3.4.R%C3%A9gimen%20de%20estimaci%C3%B3n%20directa"></string>
