          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="2.1. Principios básicos"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="10">
