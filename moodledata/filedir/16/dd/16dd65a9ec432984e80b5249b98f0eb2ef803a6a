&lt;p&gt;&lt;b&gt;CONCEPTO&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;92&quot; height=&quot;36&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;DEBE&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;92&quot; height=&quot;36&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;&lt;b&gt;HABER&lt;/b&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;/tr&gt;
&lt;tr valign=&quot;top&quot;&gt;
&lt;td height=&quot;36&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;Resultado del ejercicio&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;92&quot; height=&quot;36&quot; valign=&quot;middle&quot;&gt;
&lt;p&gt;1550&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;92&quot; height=&quot;36&quot; valign=&quot;middle&quot;&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;tr valign=&quot;top&quot;&gt;
