&lt;td width=&quot;417&quot; bgcolor=&quot;#e6e6e6&quot;&gt;
&lt;p align=&quot;center&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;Hacienda Pública, deudor por subvenciones concedidas (4708)&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;58&quot; bgcolor=&quot;#e6e6e6&quot;&gt;
&lt;p align=&quot;center&quot;&gt;&lt;/p&gt;
&lt;/td&gt;
&lt;td width=&quot;55&quot; bgcolor=&quot;#e6e6e6&quot;&gt;
&lt;p align=&quot;center&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;200&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
