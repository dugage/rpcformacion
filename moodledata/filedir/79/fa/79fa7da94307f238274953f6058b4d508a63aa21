        <unicode value=" 1. Introducción Al Metabolismo Y Al Sistema Aeróbico Y Anaeróbico De Aporte De Energía"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="5">
