        <string role="key" value="_title"></string>
        <unicode value="5. Aparato Respiratorio"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="39">
          <dictionary>
