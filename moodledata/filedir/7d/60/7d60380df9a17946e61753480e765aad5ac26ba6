&lt;p align=&quot;justify&quot;&gt;&lt;span color=&quot;#202020&quot; style=&quot;color: #202020;&quot;&gt;&lt;span face=&quot;Verdana, serif&quot; style=&quot;font-family: Verdana, serif;&quot;&gt;&lt;span&gt;&lt;b&gt;&lt;span&gt;Llevamos la cuenta del 9 al subgrupo 13 para reconocer la subvención como Patrimonio Neto:&lt;/span&gt;&lt;/b&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;table cellpadding=&quot;5&quot; cellspacing=&quot;0&quot; style=&quot;width: 515px;&quot;&gt;&lt;colgroup&gt;&lt;col width=&quot;372&quot; /&gt; &lt;col width=&quot;57&quot; /&gt; &lt;col width=&quot;54&quot; /&gt; &lt;/colgroup&gt;
