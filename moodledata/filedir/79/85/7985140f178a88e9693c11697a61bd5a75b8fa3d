        <string role="key" value="_title"></string>
        <unicode value="2. La Publicidad Registral: Naturaleza Jurídica, Objeto Y Fines"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="8">
