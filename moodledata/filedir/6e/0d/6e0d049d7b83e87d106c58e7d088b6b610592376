      <instance class="exe.engine.node.Node" reference="10">
       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="2. Tipos de vendedores"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="8">
          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="Free Text"></unicode>
           <string role="key" value="_author"></string>
