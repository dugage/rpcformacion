           <string value="exe-node:UNIDAD%20DID%C3%81CTICA%204.%20T%C3%89CNICAS%20Y%20MANIOBRAS%20DE%20LA%20PRIMERA%20ASISTENCIA%20AL%20ACCIDENTADO:4.%20Lesiones%20producidas%20por%20calor%20o%20fr%C3%ADo:4.2.%20Lesiones%20producidas%20por%20fr%C3%ADo%3A%20hipotermia%2C%20congelaciones"></string>
