           <unicode value="8.2. El mesero"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="41">
             <dictionary>
              <string role="key" value="_title"></string>
