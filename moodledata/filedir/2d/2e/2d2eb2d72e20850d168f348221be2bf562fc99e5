              <unicode value="6.1. El desayuno"></unicode>
              <string role="key" value="idevices"></string>
              <list>
               <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="35">
                <dictionary>
                 <string role="key" value="_title"></string>
