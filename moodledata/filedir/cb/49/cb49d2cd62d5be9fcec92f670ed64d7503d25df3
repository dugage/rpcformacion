          <dictionary>
           <string role="key" value="_title"></string>
           <unicode value="6.1. Impuesto General Indirecto Canario (IGIC)"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="52">
             <dictionary>
              <string role="key" value="_title"></string>
