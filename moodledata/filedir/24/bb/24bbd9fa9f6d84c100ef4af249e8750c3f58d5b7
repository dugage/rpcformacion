               rawScore++;
            }
            else
            {
               scorm.SetInteractionValue("cmi.interactions.7.result","wrong");
            }
            if (question8 == key8)
            {
               scorm.SetInteractionValue("cmi.interactions.8.result","correct");
               rawScore++;
            }
