           <string value="exe-node:M%C3%93DULO%201.%20WEDDING%20PLANNER:6.El%20protocolo%20aplicado%20a%20la%20restauracion:6.2.%20Selecci%C3%B3n%20de%20mesas%3A%20%C3%BAnicas%20o%20m%C3%BAltiple"></string>
           <string role="key" value="parent"></string>
           <reference key="47"></reference>
           <string role="key" value="top_anchors_linked_from_fields"></string>
