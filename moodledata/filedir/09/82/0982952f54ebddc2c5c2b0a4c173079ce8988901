           <string value="exe-node:UNIDAD%20DID%C3%81CTICA%208.%20T%C3%89CNICAS%20PEDAG%C3%93GICAS%20DEL%20JUEGO:2.%20An%C3%A1lisis%20y%20aplicaci%C3%B3n%20de%20los%20distintos%20juegos%20y%20recursos%20l%C3%BAdicos:2.4.%20Recursos%20l%C3%BAdicos%3A%20tipos%2C%20caracter%C3%ADsticas%20y%20aplicabilidad"></string>
           <string role="key" value="parent"></string>
           <reference key="14"></reference>
           <string role="key" value="top_anchors_linked_from_fields"></string>
