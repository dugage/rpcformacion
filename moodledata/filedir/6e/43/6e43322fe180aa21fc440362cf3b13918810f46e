            <instance class="exe.engine.node.Node" reference="118">
             <dictionary>
              <string role="key" value="_title"></string>
              <unicode value="3. Contaminantes Biológicos: Conceptos Fundamentales"></unicode>
              <string role="key" value="idevices"></string>
              <list>
               <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="116">
                <dictionary>
