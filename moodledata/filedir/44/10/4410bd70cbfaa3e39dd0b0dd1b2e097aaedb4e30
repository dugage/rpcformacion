       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="El valor de expropiación"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="196">
          <dictionary>
           <string role="key" value="_title"></string>
