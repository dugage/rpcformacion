      <instance class="exe.engine.node.Node" reference="23">
       <dictionary>
        <string role="key" value="_title"></string>
        <unicode value="6. ¿Qué Debes Hacer Si Te Lesionas Durante La Realización Del Ejercicio Físico?"></unicode>
