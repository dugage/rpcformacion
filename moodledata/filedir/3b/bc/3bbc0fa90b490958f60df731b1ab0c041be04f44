        <unicode value="9. Arquitectura Organizativa Del Climatizador Y Comunicación Con Central Gestión Motor"></unicode>
        <string role="key" value="idevices"></string>
        <list>
         <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="36">
