           <string value="exe-node:UNIDAD%20DID%C3%81CTICA%205.%20LA%20COMUNICACI%C3%93N%20EN%20LA%20EMPRESA:7.%20Herramientas%20De%20Comunicaci%C3%B3n%20Interna%20Y%20Externa:7.1.%20Herramientas%20De%20Comunicaci%C3%B3n%20Interna%20"></string>
           <string role="key" value="parent"></string>
