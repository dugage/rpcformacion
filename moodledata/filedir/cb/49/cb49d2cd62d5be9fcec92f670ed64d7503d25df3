           <string value="exe-node:Tema%203%20.%20Impuesto%20sobre%20el%20Valor%20A%C3%B1adido%20%28IVA%29.%20II:3.Tipo%20impositivo:3.2.%20Ejecuciones%20De%20Obra%20De%20Alba%C3%B1iler%C3%ADa"></string>
