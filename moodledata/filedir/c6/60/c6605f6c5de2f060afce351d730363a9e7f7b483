        <int value="2"></int>
        <string role="key" value="last_full_node_path"></string>
        <string value="exe-node:%20UNIDAD%20DID%C3%81CTICA%202.%20PROTOCOLO%20B%C3%81SICO%20Y%20NORMAS%20DE%20COMPORTAMIENTO%20EN%20RESTAURACI%C3%93N:7.%20Reglas%20b%C3%A1sicas%20de%20una%20comida"></string>
