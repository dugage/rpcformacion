                 <string role="key" value="exportType"></string>
                 <string value="scorm"></string>
                 <string role="key" value="last_full_node_path"></string>
                 <string value="exe-node:Unidad%20Did%C3%A1ctica%209.%20Minerales:Contenidos:4.Funciones%20generales%20de%20los%20minerales:Unidad%204.1%20Micronutrientes%20Mayoritarios:4.1.2.%20F%C3%B3sforo"></string>
