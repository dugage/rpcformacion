           <unicode value="2.2. Exención y no sujeción"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="21">
             <dictionary>
              <string role="key" value="_title"></string>
