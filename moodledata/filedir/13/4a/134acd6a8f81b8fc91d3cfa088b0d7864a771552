           <string role="key" value="_title"></string>
           <unicode value="2.2. Valor De Tasación De Edificios Y Elementos De Edificios Para Entidades Aseguradoras Y Fondos De Pensiones"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="12">
             <dictionary>
              <string role="key" value="_title"></string>
