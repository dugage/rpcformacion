           <string value="exe-node:UNIDAD%20DID%C3%81CTICA%204.%20EL%20EVENTO%20COMO%20PROYECTO%3A%20PLANIFICACI%C3%93N%2C%20PRESUPUESTACI%C3%93N%20Y%20ORGANIZACI%C3%93N:12.%20Presupuestaci%C3%B3n%20del%20evento:12.1.%20Presupuestos%20cerrados%20y%20presupuestos%20abiertos%20y%20sus%20caracter%C3%ADsticas"></string>
