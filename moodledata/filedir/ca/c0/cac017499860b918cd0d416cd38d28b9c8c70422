           <unicode value="5.3. Envasado activo"></unicode>
           <string role="key" value="idevices"></string>
           <list>
            <instance class="exe.engine.freetextidevice.FreeTextIdevice" reference="120">
             <dictionary>
              <string role="key" value="_title"></string>
