        <string role="key" value="_title"></string>
        <unicode value="5. Programación del evento. Tipología de actos en función de tipología de eventos: sesiones, presentaciones, descansos, comidas, ceremonias, espectáculos y exposiciones"></unicode>
        <string role="key" value="idevices"></string>
