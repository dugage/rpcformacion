           <list></list>
           <string role="key" value="exe.engine.node.Node.persistenceVersion"></string>
           <int value="2"></int>
           <string role="key" value="last_full_node_path"></string>
           <string value="exe-node:UNIDAD%20DID%C3%81CTICA%201.%20COTEJO%20DE%20DOCUMENTACI%C3%93N%20ADMINISTRATIVA%20B%C3%81SICA:3.%20Los%20Documentos%20Comerciales%20Y%20Administrativos:3.2.%20Documentos%20De%20Uso%20Interno"></string>
