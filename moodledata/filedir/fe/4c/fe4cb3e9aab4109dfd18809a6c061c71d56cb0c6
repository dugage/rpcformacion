           <string value="exe-node:TEMA%204%20%3A%20EL%20CONSUMIDOR:3.%20La%20psicolog%C3%ADa%3B%20consumidor:3.1.%20Teor%C3%ADas%20sobre%20el%20comportamiento%20del%20consumidor"></string>
