              <string value="scorm"></string>
              <string role="key" value="last_full_node_path"></string>
              <string value="exe-node:Unidad%20Did%C3%A1ctica%201.%20Valor%20nutritivo%20y%20composici%C3%B3n%20de%20los%20alimentos%20%28I%29:3.%20Alimentos%20de%20origen%20animal:3.1.%20Carne:3.1.1.%20Estructura%20de%20la%20carne"></string>
