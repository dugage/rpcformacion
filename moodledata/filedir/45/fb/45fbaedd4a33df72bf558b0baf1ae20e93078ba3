                     <instance class="exe.engine.quiztestidevice.AnswerOption">
                      <dictionary>
                       <string role="key" value="answerTextArea"></string>
                       <instance class="exe.engine.field.TextAreaField" reference="22">
                        <dictionary>
                         <string role="key" value="_id"></string>
